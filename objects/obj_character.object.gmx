        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Attack and Hit

if (keyboard_check(vk_numpad1) &amp;&amp; (gasbar &gt; 0) &amp;&amp; !gasFilling){
    if (!audio_is_playing(snd_chainsaw)) audio_play_sound(snd_chainsaw, 10, false);   
    strike = true;    
    gasbar -= 5;
}
else if((!keyboard_check(vk_numpad1) &amp;&amp; gasbar &lt;=100) || gasFilling){
    gasbar += 2;
}

if(gasbar &lt;= 0){ //If the bar hit 0, you can't attack while refilling it to 100
    gasFilling = true;
    strike = false;
}
else if(gasbar &gt;= 100){
    gasFilling = false;
}
//Hit

//Checks wich character has been hit
if(place_meeting(x,y,obj_ennemy) &amp;&amp; !strike &amp;&amp; !instance_exists(obj_magicshield)){
    ennemy = instance_place(x,y,obj_ennemy);
    hit = true;
}
if(place_meeting(obj_yellow.x, obj_yellow.y, obj_ennemy) &amp;&amp; !redprotect) {
    ennemy = instance_place(obj_yellow.x, obj_yellow.y, obj_ennemy);
    hit = true;
}

if(strike || instance_exists(obj_magicshield))
    hit = false;

if(!keyboard_check(vk_numpad1)){
    strike = false;
}
    
//Then apply the hit animation, and invulnerability
if (hit &amp;&amp; (invulnerability &lt;= 1) &amp;&amp; instance_exists(ennemy) &amp;&amp; !ennemy.death &amp;&amp; !ennemy.spawn){
    life -= ennemy.damage;
    invulnerability = room_speed*0.9;
    
    if(!place_meeting(x - sign(ennemy.image_xscale) * 25, y, obj_ground01))
        x -= sign(ennemy.image_xscale) * 25;
    y -= 25
    if(ennemy.isProjectile){
        with(ennemy) 
        instance_destroy();
    }
    if (!audio_is_playing(snd_ennemy1_hit))
        audio_play_sound(snd_ennemy1_hit, 10, false);

    if(redprotect &amp;&amp; place_meeting(obj_yellow.x, obj_yellow.y, obj_ennemy)){
            audio_play_sound(snd_shield, 10, false);
    }
}
else if (invulnerability &gt;= 0){
    hit = false;
    invulnerability--;
}


// Death
if (life &lt;= 0){
    room_goto(Game_Over);
}

</string>
